{
  "name": "Instagram",
  "version": "1.0.0",
  "description": "Extension for instagram",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["https://www.google.com/", "https://www.instagram.com/*"],
      "js": ["content.js"],
      "all_frames": true,
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": ["content.js"],
  "externally_connectable": {
    "ids": ["*"],
    "matches": ["https://www.google.com/", "https://www.instagram.com/*"]
  },
  "permissions": ["tabs", "storage"],
  "icons": {
    "16": "img/logo_16.png",
    "48": "img/logo_48.png",
    "128": "img/logo_128.png"
  },
  "browser_action": {
    "default_title": "Instagram extension",
    "default_popup": "popup.html"
  }
}
